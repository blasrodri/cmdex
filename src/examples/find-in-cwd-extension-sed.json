{
    "command": {
        "name": "find",
        "description": "Find files in current working dir with *.txt extension and execute "
    },
    "synopsis": {
        "command_options": [
            {
                "flag_values": [
                    { "flag": {}, "value": "`$(pwd)`"},
                    { "flag": {"prefix": "-", "name": "name"}, "value": "*.txt"},
                    { "flag": {"prefix": "-", "name": "exec"}, "value": "sed -i 's/foo/bar/g' {} + "}
                ]
            }
        ]
    }
}